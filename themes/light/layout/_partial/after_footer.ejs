<% if (is_post()) {%>
    <% if (theme.baidushare.enable){ %>
        <script type="text/javascript" id="bdshare_js" data="type=tools&amp;uid=<%= theme.baidushare.id %>" ></script>
        <script type="text/javascript" id="bdshell_js"></script>
        <script type="text/javascript">
        var bds_config = {
            wbUid:'<%- theme.baidushare.wbUid %>',
            snsKey:{tsina: '<%- theme.baidushare.tsina %>'}
        };
        document.getElementById("bdshell_js").src = "http://bdimg.share.baidu.com/static/js/shell_v2.js?cdnversion=" + Math.ceil(new Date()/3600000);
        </script>
    <% } %>
    <% if (theme.duoshuo && page.comments ){ %>
    <script type="text/javascript">
        var duoshuoQuery = {short_name:"<%= theme.duoshuo %>"};
        (function() {
            var ds = document.createElement('script');
            ds.type = 'text/javascript';ds.async = true;
            ds.src = 'http://static.duoshuo.com/embed.js';
            ds.charset = 'UTF-8';
            (document.getElementsByTagName('head')[0]
            || document.getElementsByTagName('body')[0]).appendChild(ds);
        })();
    </script>
    <% } %>
<% } %>

